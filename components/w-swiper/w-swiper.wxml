<!--components/w-swiper/w-swiper.wxml-->
<swiper class="swiper" 	
        circular
        autoplay
        interval="3000"
        duration="300"
        indicator-active-color="#ff5777"
        indicator-dots>
  <block wx:for="{{list}}" wx:key="index">
    <swiper-item class="swiper-item">
      <image src="{{item.image || item}}"></image>
    </swiper-item>
  </block>
</swiper>
